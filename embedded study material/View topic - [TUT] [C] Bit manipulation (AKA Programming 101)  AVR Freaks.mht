                                port &amp;=3D ~(1&lt;&lt;pin) =
<BR>#define=20
                                OUTPUT(port,pin) DDR ## port |=3D =
(1&lt;&lt;pin)=20
                                <BR>#define CLEAR(port,pin) PORT ## port =
&amp;=3D=20
                                ~(1&lt;&lt;pin) <BR>#define =
SET(port,pin) PORT=20
                                ## port |=3D (1&lt;&lt;pin) <BR>#define=20
                                TOGGLE(port,pin) PORT ## port ^=3D =
(1&lt;&lt;pin)=20
                                <BR>#define READ(port,pin) (PIN ## port =
&amp;=20
                                (1&lt;&lt;pin)) <BR></DIV><BR><BR>usage: =
