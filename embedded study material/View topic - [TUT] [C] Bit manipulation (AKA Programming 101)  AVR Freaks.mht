<BR>#define=20
                                BITFROMCOMB(x, y) y <BR><BR>#define=20
                                C_SETBIT(comb) SETBIT(VARFROMCOMB(comb), =

                                BITFROMCOMB(comb)) <BR>#define =
C_CLEARBIT(comb)=20
